copado -p 'Cloning the repository' 
mkdir MetadataRepo 
cd MetadataRepo 
git clone "$metadataRepo" .
cd ..
copado -p 'Creating SFDX Project' 
sfdx force:project:create -n SourceProject
copado -p 'Cloning Metadata to Source Project'
cd SourceProject
mkdir src
cd src
echo '<?xml version="1.0" encoding="UTF-8"?>' > package.xml
echo '<Package xmlns="http://soap.sforce.com/2006/04/metadata">' >> package.xml
echo '<types>' >> package.xml
echo '<members>*</members>' >> package.xml
echo '<name>CustomObject</name>' >> package.xml
echo '</types>' >> package.xml
echo '<version>52.0</version>' >> package.xml
echo '</Package>' >> package.xml
echo "before cat"
cat package.xml
echo "after cat"
cd ../..
cp -a MetadataRepo/objects SourceProject/src/
#cp -a MetadataRepo/* SourceProject/src/
cd SourceProject
sfdx force:mdapi:convert -r src/ -d force-app/
#rmdir src
#rm package.xml
git config --global user.email "$sourceRepoEmail"
git config --global user.name "$sourceRepoUName"
ls force-app/main/default/objects/Bicycle__c
copado -p 'Initializing and Setting Repo'
git init
git add .
copado -p 'Commiting Source Project'
git commit -m 'Updated Source Format'
git remote add origin $sourceRepo
copado -p 'Pushing to Source Project'
git push origin master